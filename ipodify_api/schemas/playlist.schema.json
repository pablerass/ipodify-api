{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "owner": {
      "type": "string"
    },
    "visibility": {
      "type": "string",
      "enum": ["private"],
      "default": "private"
    },
    "filter": {
      "$ref": "filter.schema.json"
    }
  },
  "requiredProperties": ["name", "owner", "filter"]
}